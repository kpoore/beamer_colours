% TARDIS Beamer colour theme v0.8
% Keith Poore
% 1/6/2013
% Dark Blue background, Black Text background, white fore ground text
%
%Works with: AnnArbor, Antibes, Bergen, Berlin, Berkeley, Boadilla, CambridgeUS, Copenhagen, Darmstadt, Dresden, Ilmenau, Madrid, Rochester, Warsaw, Goettingen (buttons), Hannover (buttons), Singapore (buttons), Szeged (buttons)
%
%Does not work with: Frankfurt, 
%

\mode<presentation>

\definecolor{tardisblue}{RGB}{16,35,114}
%%\definecolor{}{RGB}{,,}

\setbeamercolor{alerted text}{fg=white}
\setbeamercolor{background canvas}{bg=tardisblue}

\setbeamercolor{block body alerted}{bg=tardisblue}
\setbeamercolor{block body}{bg=tardisblue}
\setbeamercolor{block body example}{bg=tardisblue}
\setbeamercolor{block title alerted}{bg=tardisblue, fg=white}
\setbeamercolor{block title}{bg=tardisblue, fg=white}
\setbeamercolor{block title example}{bg=tardisblue, fg=white}

\setbeamercolor{fine separation line}{bg=black}
\setbeamercolor{frametitle}{fg=white}
\setbeamercolor{item projected}{bg=tardisblue, fg=black}
\setbeamercolor{normal text}{bg=black,fg=white}

\setbeamercolor{palette primary}{bg=white, fg=black}
\setbeamercolor{palette secondary}{bg=white,fg=black}
\setbeamercolor{palette tertiary}{fg=tardisblue,bg=white}
\setbeamercolor{palette quaternary}{bg=tardisblue,fg=white}

\setbeamercolor{palette sidebar primary}{bg=white, fg=tardisblue}
\setbeamercolor{palette sidebar quaternary}{bg=white,fg=black}
\setbeamercolor{palette sidebar secondary}{bg=white,fg=black}
\setbeamercolor{palette sidebar tertiary}{bg=tardisblue,fg=white}

\setbeamercolor{section in sidebar}{fg=white}
\setbeamercolor{section in sidebar shaded}{fg=white}

\setbeamercolor{separation line}{fg=tardisblue, bg=white}
\setbeamercolor{sidebar}{bg=tardisblue, fg=white}
\setbeamercolor{structure}{fg=tardisblue, bg=white}

\setbeamercolor{subsection in sidebar}{bg=tardisblue, fg=white}
\setbeamercolor{subsection in sidebar shaded}{bg=tardisblue, fg=white}

\setbeamercolor{itemize item}{bg=tardisblue, fg=white}
\setbeamercolor{enumerate item}{bg=tardisblue, fg=white}

\setbeamercolor{title}{bg=tardisblue, fg=white}
\setbeamercolor{titlelike}{bg=tardisblue, fg=white}
\setbeamercolor{title in sidebar}{bg=tardisblue, fg=white}

\setbeamercolor{logo}{bg=tardisblue}

\setbeamercolor{frametitle}{bg=tardisblue, fg=white}

\setbeamertemplate{blocks}[rounded][shadow=false]
\setbeamertemplate{background canvas}[vertical shading][bottom=tardisblue,top=tardisblue]
\setbeamertemplate{sidebar canvas left}[horizontal shading][left=tardisblue,right=black]

\beamertemplatenavigationsymbolsempty
\mode
<all>
