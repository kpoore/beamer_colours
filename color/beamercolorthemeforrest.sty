% Forrest Beamer colour theme v0.9
% Keith Poore
% 3/6/2013
% Dark Green background, Light Brown Text background, light green text, light brown
%
%Works with: AnnArbor, Antibes, Bergen, Berlin, Berkeley, Boadilla, CambridgeUS, Copenhagen, Darmstadt, Dresden, Frankfurt, Hannover, Ilmenau, Madrid, Rochester, Singapore, Szeged, Warsaw 
%
%
%Does not work with:Goettingen
%

\mode<presentation>

%%%%%   Definition of colours
\definecolor{treebrown}{RGB}{80,42,14}
\definecolor{leafgreen}{RGB}{6,185,54}
\definecolor{chlorogreen}{RGB}{2,105,5}
\definecolor{branchbrown}{RGB}{158,88,35}

%%%%   Set colours to elements
\setbeamercolor{palette primary}{fg=treebrown, bg=leafgreen}
\setbeamercolor{palette secondary}{fg=treebrown, bg=leafgreen}
\setbeamercolor{palette tertiary}{fg=treebrown, bg=leafgreen}
\setbeamercolor{palette quaternary}{fg=treebrown, bg=leafgreen}

\setbeamercolor{palette sidebar primary}{bg=leafgreen, fg=branchbrown}
\setbeamercolor{palette sidebar secondary}{bg=leafgreen, fg=branchbrown}
\setbeamercolor{palette sidebar tertiary}{bg=leafgreen, fg=branchbrown}
\setbeamercolor{palette sidebar quaternary}{bg=leafgreen, fg=branchbrown}

\setbeamercolor{alerted text}{fg=black} %% "current" text
\setbeamercolor{background canvas}{bg=chlorogreen}%%
\setbeamercolor{background}{bg=chlorogreen}

\setbeamercolor{block body alerted}{bg=leafgreen}
\setbeamercolor{block body}{bg=leafgreen}
\setbeamercolor{block body example}{bg=treebrown}
\setbeamercolor{block title alerted}{bg=leafgreen, fg=black}
\setbeamercolor{block title}{bg=treebrown, fg=black}
\setbeamercolor{block title example}{bg=leafgreen, fg=black}

\setbeamercolor{fine separation line}{}

\setbeamercolor{item projected}{bg=chlorogreen, fg=black}
\setbeamercolor{normal text}{fg=black}

\setbeamercolor{section in sidebar}{fg=branchbrown}
\setbeamercolor{section in sidebar shaded}{bg = branchbrown, fg=white}
\setbeamercolor{subsection in sidebar}{fg=white}
\setbeamercolor{subsection in sidebar shaded}{bg=chlorogreen, fg=white}

\setbeamercolor{separation line}{}
\setbeamercolor{sidebar}{fg=black, bg=branchbrown}
\setbeamercolor{structure}{fg=black, bg=branchbrown}

\setbeamercolor{itemize item}{fg=branchbrown}
\setbeamercolor{enumerate item}{bg=treebrown, fg=black}

\setbeamercolor{title}{bg=branchbrown, fg=black}
\setbeamercolor{subtitle}{fg=black, bg=leafgreen}
\setbeamercolor{title page}{fg=black, bg=chlorogreen}
\setbeamercolor{titlelike}{bg=leafgreen, fg=white}
\setbeamercolor{title in sidebar}{fg=black}

\setbeamercolor{logo}{bg=branchbrown}

\setbeamercolor{frametitle}{bg=branchbrown, fg=black}

%\setbeamertemplate{blocks}[rounded][shadow=false]
%\setbeamertemplate{background canvas}[vertical shading][bottom=white,top=black]
%\setbeamertemplate{sidebar canvas left}[horizontal shading][left=treebrown,right=black]

\beamertemplatenavigationsymbolsempty
\mode
<all>
